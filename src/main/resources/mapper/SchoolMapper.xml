<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.rgtask.mapper.SchoolMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.rgtask.pojo.School">
        <id column="id" property="id" />
        <result column="country" property="country" />
        <result column="privince" property="privince" />
        <result column="city" property="city" />
        <result column="schoolname" property="schoolname" />
        <result column="create_date" property="createDate" />
        <result column="update_time" property="updateTime" />
        <result column="del_flag" property="delFlag" />
        <result column="remark" property="remark" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, country, privince, city, schoolname, create_date, update_time, del_flag, remark
    </sql>

</mapper>
